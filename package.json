{
  "name": "zgwnu-ng2-bonita-testapp",
  "version": "0.0.1-alpha-2",
  "description": "Angular 2 Library with bonita BPM Rest Api Test Application",

  "scripts": {
    "start": "concurrently \"tsc -w\" \"lite-server\" ",
    "prestart": "grunt clean && tsc",
    "postinstall:mac": "grunt --gruntfile Grunt-post-install-bonita.js postinstall_mac",
    "postinstall:win": "grunt --gruntfile Grunt-post-install-bonita.js postinstall_win",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "pree2e": "webdriver-manager update",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "init:aot": "grunt clean:aot && ngc -p tsconfig-aot-init.json && grunt aot_init",
    "ngc:aot": "ngc -p tsconfig-aot.json",
    "ngc:prod": "ngc -p tsconfig-aot-prod.json",
    "rollup:aot": "rollup -c rollup-config.js",
    "rollup:prod": "rollup -c rollup-config-prod.js",
    "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
    "build:prod": "ngc -p tsconfig-aot-prod.json && rollup -c rollup-config-prod.js",
    "map:aot": "source-map-explorer aot/dist/build.js",
    "lite:aot": "lite-server -c aot/bs-config.json",
    "build:war": "grunt --gruntfile Grunt-build-war.js",
    "local:war": "grunt --gruntfile Grunt-build-war.js && grunt --gruntfile Grunt-deploy-local.js && grunt localapp"
  },

  "keywords": ["BPM", "ZgW.Nu", "ZaakgerichtWerken.Nu", "BonitaBPM", "Angular 2"],
  "author": "Onno Haldar",
  "license": "GPL-3.0",

  "dependencies": {
    "@angular/common": "~2.4.8",
    "@angular/compiler": "~2.4.8",
    "@angular/compiler-cli": "^2.4.8",
    "@angular/core": "~2.4.8",
    "@angular/forms": "~2.4.8",
    "@angular/http": "~2.4.8",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/platform-browser": "~2.4.8",
    "@angular/platform-browser-dynamic": "~2.4.8",
    "@angular/platform-server": "^2.4.8",
    "@angular/router": "~3.4.8",
    "angular-in-memory-web-api": "~0.3.0",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "reflect-metadata": "^0.1.10",
    "rxjs": "5.2.0",
    "systemjs": "0.20.9",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.43",
    "@types/node": "^7.0.5",
    "@types/selenium-webdriver": "^2.53.40",
    "canonical-path": "0.0.2",
    "concurrently": "^3.3.0",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-mkdir": "^1.0.0",
    "grunt-open": "^0.2.3",
    "grunt-shell": "^2.1.0",
    "grunt-text-replace": "^0.4.0",
    "grunt-tomcat-deploy": "^0.1.7",
    "grunt-war": "^0.5.1",
    "grunt-zip": "^0.17.1",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "cors": "^2.8.1",
    "lodash": "^4.17.4",
    "protractor": "5.1.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.4",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "source-map-explorer": "^1.3.3",
    "tslint": "^4.5.1",
    "typescript": "~2.0.10",
    "webdriver-manager": "12.0.2"
  },

  "repository": { 
    "type" : "git", 
    "url" : "https://github.com/onnohaldar/zgwnu-ng2-bonita-testapp.git"
  }
}
